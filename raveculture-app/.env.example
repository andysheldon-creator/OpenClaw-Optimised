# =============================================================================
# RaveCulture App — Environment Variables
# =============================================================================
# Copy to .env.local and fill in values. Never commit .env.local.

# --- OnchainKit / Coinbase ---
NEXT_PUBLIC_ONCHAINKIT_API_KEY=

# --- Bankr SDK ---
# Public API key (safe for client config references, but Bankr runs server-only)
NEXT_PUBLIC_BANKR_API_KEY=

# Private key for server-side Bankr signing — NEVER expose client-side
BANKR_PRIVATE_KEY=

# --- Clanker Chat ---
NEXT_PUBLIC_CLANKER_KEY=

# --- Token Gating ---
# Contract address for RavePass / gating token
RAVECULTURE_TOKEN_ADDRESS=

# --- WalletConnect (wagmi) ---
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=
